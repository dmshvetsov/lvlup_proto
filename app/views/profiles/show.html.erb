<div class="row">
  <div class="col-lg-12">
    <h2>List of skills</h2>
    <ul class="list-group">
      <% @profile.characteristics.each do |char| %>
        <li class="list-group-item">
          <%= link_to char.title, characteristics_path(char) %>
          <span class="label label-default"><%= char.lvlups.count %></span>
        </li>
      <% end %>
    </ul>
  </div>
</div>
